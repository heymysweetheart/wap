<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Quiz</servlet-name>
        <servlet-class>mum.cs472.QuizServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Quiz</servlet-name>
        <url-pattern>/quiz</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QuizNew</servlet-name>
        <servlet-class>mum.cs472.QuizServletNew</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QuizNew</servlet-name>
        <url-pattern>/newquiz</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BearSelector</servlet-name>
        <servlet-class>mum.cs472.BeerSelect</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>BearSelector</servlet-name>
        <url-pattern>/selectbeer</url-pattern>
    </servlet-mapping>
</web-app>